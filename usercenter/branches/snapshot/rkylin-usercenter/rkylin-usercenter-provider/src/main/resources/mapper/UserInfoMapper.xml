   <select id="selectByLoginName" resultMap="BaseResultMap"  >
  
  <!--  SELECT a.*,b.USER_CNAME,b.ID_NUMBER, b.ETHNIC,b.ID_NUMBER_ADDRESS,b.ID_NUMBER_EXPIRE_TIME,b.NATIONAL,c.ID_CARD_NUMBER,c.IS_IDENTIFY,c.USER_ID,c.USER_CODE,b.MOBILE_PHONE,b.FIXED_PHONE,b.EMAIL from UC_USER_LOGIN a LEFT JOIN UC_PERSON_INFO b
    ON a.USER_INFO_ID=b.USER_INFO_ID LEFT JOIN UC_USER_INFO c  ON a.USER_INFO_ID=c.USER_INFO_ID  WHERE a.LOGIN_NAME=#{loginName,jdbcType=VARCHAR}  and a.ORG_CODE=#{orgCode,jdbcType=VARCHAR}
 -->   
   SELECT a.*,b.*,b.COMPANY_NAME as PCOMPANY_NAME,d.*,d.COMPANY_NAME as CCOMPANY_NAME,c.ID_CARD_NUMBER,c.IS_IDENTIFY,c.USER_ID,c.USER_CODE,c.STATUS,c.CREATED_TIME,c.UPDATED_TIME from UC_USER_LOGIN a LEFT JOIN UC_PERSON_INFO b
    ON a.USER_INFO_ID=b.USER_INFO_ID  LEFT JOIN UC_COMPANY_INFO  d
    ON a.`USER_INFO_ID`=d.USER_INFO_ID LEFT JOIN UC_USER_INFO c  ON a.USER_INFO_ID=c.USER_INFO_ID  WHERE a.LOGIN_NAME=#{loginName,jdbcType=VARCHAR}  and a.ORG_CODE=#{orgCode,jdbcType=VARCHAR}
   
  </select>
  
   <select id="selectByLoginNameAndUserType" resultMap="BaseResultMap"  >
  
   
   SELECT a.*,
   <if test="1==userType" >
       d.*,
   </if>
   <if test="2==userType" >
       b.*,
   </if>
    c.ID_CARD_NUMBER,c.IS_IDENTIFY,c.USER_ID,c.USER_CODE from UC_USER_LOGIN a LEFT JOIN UC_PERSON_INFO b
    ON a.USER_INFO_ID=b.USER_INFO_ID  LEFT JOIN UC_COMPANY_INFO  d
    ON a.`USER_INFO_ID`=d.USER_INFO_ID LEFT JOIN UC_USER_INFO c  ON a.USER_INFO_ID=c.USER_INFO_ID  WHERE a.LOGIN_NAME=#{loginName,jdbcType=VARCHAR}  and a.ORG_CODE=#{orgCode,jdbcType=VARCHAR}
   
  </select>
   
