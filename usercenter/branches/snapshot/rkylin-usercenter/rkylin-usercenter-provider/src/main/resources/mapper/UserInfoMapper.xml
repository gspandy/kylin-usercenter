  </select>
 
  <select id="selectByUserId" resultMap="BaseResultMap"  >
  
   SELECT * from  UC_USER_INFO   WHERE  USER_ID = #{userId,jdbcType=VARCHAR} and ORG_CODE= #{orgCode} 
   
  </select>
  
   <select id="selectByUserIdAndUserType" resultMap="BaseResultMap"  >
  
   
   SELECT c.*,
       d.*,
       b.*,
 a.ID_CARD_NUMBER,
a.IS_IDENTIFY,
a.USER_ID,
a.USER_CODE FROM 
UC_USER_INFO
 a 
LEFT JOIN UC_PERSON_INFO b 
  ON a.USER_INFO_ID = b.USER_INFO_ID 
LEFT JOIN UC_COMPANY_INFO d 
  ON a.`USER_INFO_ID` = d.USER_INFO_ID 
LEFT JOIN UC_USER_LOGIN c 
  ON a.USER_INFO_ID = c.USER_INFO_ID 
    
    
     WHERE a.USER_ID=#{userId,jdbcType=VARCHAR}  and a.ORG_CODE=#{orgCode,jdbcType=VARCHAR}
   
  </select>
